@namespace Bdziam.UI
@inherits BButtonBase

<button id="@ButtonId" class="@ButtonClasses" style="@ButtonStyles" @onclick="ToggleState" disabled="@Disabled">
    <BRipple>

    @if (StartIcon != null)
    {
        <Blazicon Svg="@StartIcon"/>
    }
    @ChildContent
    @if (EndIcon != null)
    {
        <Blazicon Svg="@EndIcon"/>
    }
    </BRipple>
</button>
